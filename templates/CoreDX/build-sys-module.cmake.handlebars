function(add_{{addonName}}_sources TARGET_NAME GEN_ROOT_PATH)
    target_sources(
        ${TARGET_NAME}
        PUBLIC
        {{#each sourceFiles}}
            "${GEN_ROOT_PATH}/{{this}}"
        {{/each}}
    )
endfunction(add_{{addonName}}_sources TARGET_NAME GEN_ROOT_PATH)

